tasks = ['job hunting', 'coding', 'reading', 'working out', 'free time']
tasks[1] = 'crying'
print(tasks [1:3])


flowers = ['dandelion', 'orchid', 'peony', 'rose', 'lily', 'lotus']
suspicious_numbers = [6, 17, 26, 31, 1042, 60941]
flowers.extend(suspicious_numbers)
print(flowers)
print(suspicious_numbers)


flowers = ['dandelion', 'orchid', 'peony', 'rose', 'lily', 'lotus']
suspicious_numbers = [6, 17, 26, 31, 1042, 60941]
flowers.append('baby\'s breath')
print(flowers)
print(suspicious_numbers)


flowers = ['dandelion', 'orchid', 'peony', 'rose', 'lily', 'lotus']
suspicious_numbers = [6, 17, 26, 31, 1042, 60941]
flowers.insert(2, 'tulip')
print(flowers)
print(suspicious_numbers)


